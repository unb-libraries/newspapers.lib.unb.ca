{
  "authors": [
    {
      "name": "UNB Libraries libsupport@unb.ca",
      "role": "Creator"
    }
  ],
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "php-http/discovery": true
    },
    "discard-changes": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "conflict": {
    "drupal/ctools": "^4.0.0",
    "drupal/drupal": "*"
  },
  "description": "Drupal 8 component of newspapers.lib.unb.ca",
  "extra": {
    "enable-patching": true,
    "installer-paths": {
      "core": [
        "type:drupal-core"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ],
      "libraries/{$name}": [
        "type:drupal-library"
      ],
      "modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "themes/contrib/{$name}": [
        "type:drupal-theme"
      ]
    },
    "patches": {
      "drupal/address": {
        "Integrate Address with Search API": "https://www.drupal.org/files/issues/2022-08-11/address-n2812659-37-fix.patch"
      },
      "drupal/core": {
        "Exposed filter values available as tokens": "https://www.drupal.org/files/issues/2022-10-30/2761273-50.patch",
        "Extend Daterange functionality": "https://www.drupal.org/files/issues/2794481-50.patch",
        "HTML5 validation issue": "https://www.drupal.org/files/issues/2018-07-13/1797438-145.patch",
        "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch"
      },
      "drupal/elasticsearch_connector": {
        "Cannot combine elasticsearch with other backends": "https://www.drupal.org/files/issues/elasticsearch_connector-fix-serialize-closure-error-2904033-9.patch",
        "Cannot parse content access": "https://www.drupal.org/files/issues/2018-06-07/elasticsearch_connector-convert_boolean_fields-2978005-2.patch"
      }
    }
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "unb-libraries/newspapers.lib.unb.ca",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip"
        },
        "name": "harvesthq/chosen",
        "require": {
          "composer/installers": "^1.2.0"
        },
        "type": "drupal-library",
        "version": "1.8.7"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/FortAwesome/Font-Awesome/archive/refs/tags/5.15.4.zip"
        },
        "extra": {
          "installer-name": "fontawesome"
        },
        "name": "fortawesome/font-awesome",
        "type": "drupal-library",
        "version": "5.15.4"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/furf/jquery-ui-touch-punch/archive/refs/heads/master.zip"
        },
        "name": "furf/jquery-ui-touch-punch",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/simeydotme/jQuery-ui-Slider-Pips/archive/v1.11.4.zip"
        },
        "name": "simeydotme/jquery-ui-slider-pips",
        "require": {
          "composer/installers": "^1.2.0"
        },
        "type": "drupal-library",
        "version": "1.11.4"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://github.com/markgoodyear/scrollup/archive/refs/heads/master.zip"
        },
        "extra": {
          "installer-name": "scrollup"
        },
        "name": "markgoodyear/scrollup",
        "type": "drupal-library",
        "version": "dev-master"
      },
      "type": "package"
    }
  ],
  "require": {
    "drupal/address": "1.11",
    "drupal/allowed_formats": "1.5",
    "drupal/antibot": "2.0.2",
    "drupal/bootstrap_barrio": "5.1.7",
    "drupal/chosen": "3.0.5",
    "drupal/conditional_fields": "4.0.0-alpha1",
    "drupal/core": "9.5.5",
    "drupal/easy_breadcrumb": "2.0.3",
    "drupal/externalauth": "1.4",
    "drupal/facets": "2.0.6",
    "drupal/field_group": "3.4",
    "drupal/fontawesome": "2.24",
    "drupal/google_analytics": "2.5",
    "drupal/health_check": "1.3",
    "drupal/jquery_scrollup": "1.0",
    "drupal/jquery_ui": "1.6",
    "drupal/jquery_ui_datepicker": "1.4",
    "drupal/jquery_ui_slider": "1.1",
    "drupal/jquery_ui_touch_punch": "1.1.0",
    "drupal/languagefield": "1.9",
    "drupal/migrate_plus": "5.3",
    "drupal/migrate_source_csv": "3.5",
    "drupal/migrate_tools": "5.2",
    "drupal/noreqnewpass": "1.3",
    "drupal/override_node_options": "2.6",
    "drupal/owlcarousel": "1.0-beta1",
    "drupal/pathauto": "1.11",
    "drupal/redis": "1.6",
    "drupal/samlauth": "3.8",
    "drupal/search_api": "1.28",
    "drupal/search_api_autocomplete": "1.7",
    "drupal/search_api_solr": "4.2.10",
    "drupal/token": "1.11",
    "drupal/twig_field_value": "2.0.2",
    "drupal/twig_tweak": "2.10",
    "drupal/view_unpublished": "1.0",
    "drupal/views_data_export": "1.2",
    "drupal/views_infinite_scroll": "1.9",
    "drupal/webform": "6.1.4",
    "fortawesome/font-awesome": "5.15.4",
    "furf/jquery-ui-touch-punch": "dev-master",
    "harvesthq/chosen": "~1.8",
    "markgoodyear/scrollup": "dev-master",
    "neitanod/forceutf8": "^2",
    "nodespark/des-connector": "5.x-dev",
    "phpoffice/phpspreadsheet": "^1.5.0",
    "simeydotme/jquery-ui-slider-pips": "~1.11",
    "unb-libraries/alert_scheduler": "dev-8.x-1.x",
    "unb-libraries/calendar_hours": "dev-9.x-1.x",
    "unb-libraries/drupal-builder": "^9",
    "unb-libraries/saml_features": "dev-9.x-1.x",
    "unb-libraries/unb_lib_theme": "dev-9.x-4.x"
  },
  "require-dev": {
    "unb-libraries/drupal-development-libraries": "^9"
  },
  "scripts": {
    "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "post-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
    ],
    "pre-install-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
    ]
  },
  "type": "project"
}
