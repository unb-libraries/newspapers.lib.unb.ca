{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Project definition for newspapers.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/conditional_fields": {
                "Conditional fields Uninitialized string offset": "https://www.drupal.org/files/issues/conditional_fields-uninitialized_string_offset_in_conditional_fields_evaluate_dependency-2834358-11.patch"
            },
            "drupal/core": {
                "Extend Daterange functionality": "https://www.drupal.org/files/issues/2794481-50.patch",
                "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch",
                "To display exposed filter block titles": "https://www.drupal.org/files/issues/2720101-38.patch"
            },
            "drupal/elasticsearch_connector": {
                "Cannot combine elasticsearch with other backends": "https://www.drupal.org/files/issues/elasticsearch_connector-fix-serialize-closure-error-2904033-9.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "unblibraries/newspapers.lib.unb.ca",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/unb_lib_theme.git"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-development-libraries.git"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.2/chosen_v1.8.2.zip"
                },
                "name": "harvesthq/chosen",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.8.2"
            },
            "type": "package"
        }
    ],
    "require": {
        "commerceguys/addressing": "1.x-dev",
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "1.6.3",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/address": "1.3",
        "drupal/bootstrap": "3.11",
        "drupal/chosen": "2.5",
        "drupal/conditional_fields": "1.0-alpha4",
        "drupal/config_update": "1.5",
        "drupal/console": "~1.0",
        "drupal/core": "8.5.3",
        "drupal/elasticsearch_connector": "5.0-alpha2",
        "drupal/externalauth": "1.1",
        "drupal/features": "3.7",
        "drupal/field_group": "3.0-beta1",
        "drupal/google_analytics": "2.2",
        "drupal/languagefield": "1.4",
        "drupal/ldap": "3.0-beta3",
        "drupal/material_admin": "1.0-alpha6",
        "drupal/pathauto": "1.2",
        "drupal/redis": "1.0-rc2",
        "drupal/search_api": "1.8",
        "drupal/token": "1.1",
        "drush/drush": "^8.1.15",
        "harvesthq/chosen": "~1.8",
        "nodespark/des-connector": "5.x-dev",
        "unb-libraries/unb_lib_theme": "dev-8.x-bootstrap-3.x",
        "webflo/drupal-finder": "^0.3.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
