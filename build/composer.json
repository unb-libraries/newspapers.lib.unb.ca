{
    "authors": [
        {
            "name": "UNB Libraries libsupport@unb.ca",
            "role": "Creator"
        }
    ],
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        }
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "description": "Drupal 8 component of newspapers.lib.unb.ca",
    "extra": {
        "enable-patching": true,
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ]
        },
        "patches": {
            "drupal/address": {
                "Integrate Address with Search API": "https://www.drupal.org/files/issues/2018-06-07/address-search-api-full-name-properties-2812659-37.patch"
            },
            "drupal/conditional_fields": {
                "Conditional fields Uninitialized string offset": "https://www.drupal.org/files/issues/conditional_fields-uninitialized_string_offset_in_conditional_fields_evaluate_dependency-2834358-11.patch"
            },
            "drupal/core": {
                "Extend Daterange functionality": "https://www.drupal.org/files/issues/2794481-50.patch",
                "HTML5 validation issue": "https://www.drupal.org/files/issues/2018-07-13/1797438-145.patch",
                "To correct DerivativeDiscoveryDecorator warning": "https://www.drupal.org/files/issues/patch_empty_fix.patch",
                "To display exposed filter block titles": "https://www.drupal.org/files/issues/2720101-38.patch",
                "Exposed filter values available as tokens": "https://www.drupal.org/files/issues/2018-05-14/make_exposed_filter-2761273-13.patch"
            },
            "drupal/elasticsearch_connector": {
                "Cannot combine elasticsearch with other backends": "https://www.drupal.org/files/issues/elasticsearch_connector-fix-serialize-closure-error-2904033-9.patch",
                "Cannot parse content access": "https://www.drupal.org/files/issues/2018-06-07/elasticsearch_connector-convert_boolean_fields-2978005-2.patch"
            },
            "drupal/facets": {
                "Titles do not show for slider facets": "https://www.drupal.org/files/issues/2018-10-18/3002580_6.patch"
            }
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "name": "unb-libraries/newspapers.lib.unb.ca-drupal8",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/unb_lib_theme.git"
        },
        {
            "type": "git",
            "url": "https://github.com/unb-libraries/drupal-development-libraries.git"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip"
                },
                "name": "harvesthq/chosen",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.8.2"
            },
            "type": "package"
        },
        {
            "package": {
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/simeydotme/jQuery-ui-Slider-Pips/archive/v1.11.3.zip"
                },
                "name": "simeydotme/jquery-ui-slider-pips",
                "require": {
                    "composer/installers": "^1.2.0"
                },
                "type": "drupal-library",
                "version": "1.11.3"
            },
            "type": "package"
        }
    ],
    "require": {
        "composer/installers": "^1.2.0",
        "cweagans/composer-patches": "1.6.3",
        "drupal-composer/drupal-scaffold": "~2.2.0",
        "drupal/address": "1.4",
        "drupal/bootstrap": "3.13",
        "drupal/chosen": "2.6",
        "drupal/conditional_fields": "1.0-alpha4",
        "drupal/config_update": "1.5",
        "drupal/console": "~1.0",
        "drupal/core": "8.6.3",
        "drupal/externalauth": "1.1",
        "drupal/facets": "1.1",
        "drupal/features": "3.8",
        "drupal/field_group": "3.0-beta1",
        "drupal/google_analytics": "2.3",
        "drupal/languagefield": "1.4",
        "drupal/ldap": "3.0-beta3",
        "drupal/material_admin": "1.0-alpha6",
        "drupal/owlcarousel": "1.0-alpha2",
        "drupal/pathauto": "1.3",
        "drupal/restui": "1.15",
        "drupal/search_api": "1.10",
        "drupal/search_api_autocomplete": "1.1",
        "drupal/search_api_solr": "2.2",
        "drupal/token": "1.5",
        "drupal/twig_field_value": "1.1",
        "drupal/twig_tweak": "2.1",
        "drush/drush": "^9.5.1",
        "harvesthq/chosen": "~1.8",
        "nodespark/des-connector": "5.x-dev",
        "phpoffice/phpspreadsheet": "^1.5.0",
        "simeydotme/jquery-ui-slider-pips": "~1.11",
        "unb-libraries/unb_lib_theme": "dev-8.x-bootstrap-3.x",
        "webflo/drupal-finder": "^1.1",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "unb-libraries/drupal-development-libraries": "^8.0.0"
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ]
    },
    "type": "project"
}
